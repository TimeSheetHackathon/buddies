{
  "manifest_version": 2,
  "name": "Fill Timesheets",
  "version": "0.1",
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["lib/jquery-3.1.1.min.js","js/actions.js"]
  }
],
  "browser_action": {
  "default_icon": "img/tw.png",
  "default_title": "Missing Sheets",
  "default_popup": "html/login.html"
  },
  "background": {
  "scripts": ["js/background.js"]
  },
  "permissions": [
    "storage",
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}